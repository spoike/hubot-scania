module.exports = (robot) ->
  getConfig = () ->
    robot.brain.get("hubot-terrascania") or {}
  setRoom = (room, enabled) ->
    config = getConfig()
    config[room] = enabled
    robot.brain.set("hubot-terrascania", config)
  isEnabled = (msg) ->
    getConfig()[msg.message.room]
  robot.respond /scania (on|yo)/i, (msg) ->
    config = getConfig
    setRoom msg.message.room, true
  robot.respond /scania (off|stfu)/i, (msg) ->
    config = getConfig
    setRoom msg.message.room, false
  robot.hear /sk(au|å)n(e|sk[at]?|ing(ar)?)/i, (msg) ->
    msg.send msg.random scania if isEnabled(msg)
  robot.hear /(^lund|[\s\W]lund)/i, (msg) ->
    msg.send msg.random scania if isEnabled(msg)
